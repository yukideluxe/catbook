<!DOCTYPE html>
<html>
  <head>
    <title>Catbook</title>
    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
    <%= csrf_meta_tags %>
  </head>
  <body>
    <div class="container">
      <% flash.map do |key, msg| %>
        <%= content_tag :div, msg, id: key %>
      <% end %>

      <%= yield %>
    </div>
  </body>
</html>
